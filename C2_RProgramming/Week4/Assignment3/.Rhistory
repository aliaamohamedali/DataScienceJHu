subset_lower_limit <- function(vec, limit){
subset <- vec>limit
vec[subset]
}
subset_lower_limit(c(1:10), 5)
subset_lower_limit <- function(vec, limit=5){
subset <- vec>limit
vec[subset]
}
subset_lower_limit(1:10)
column_mean <- function(mat){
means <- numeric(ncol(mat))
for(i in 1:ncol(mat)){
means[i] <- mean(mat[, i])
}
means
}
